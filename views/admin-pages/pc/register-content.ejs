                    <div class="col-md-3 col-sm-12  form-group">
                        <div class="form-group row">
                            <label>T-shirst Size *</label>
                            <select class="form-control" id="m_tshirt1" name="m_tshirt1">
                                <option disabled>Choose an Option</option>
                                <option value="M">Medium</option>
                                <option value="XL">Extra Large</option>
                                <option value="L">Large</option>
                                <option value="S">Small</option>
                            </select>
                        </div>
                    </div>
                </div>

                <!--Team Member 2-->
                <div class="x_title">
                    <h6>Team Member-2 Information</h6>                    
                    <div class="clearfix"></div>
                </div>

                <div class="item form-group">
                    <div class="col-md-3 col-sm-12  form-group">
                        <div class="form-group row">
                            <label>Name of Member-2 *</label>
                            <input required="required" type="text" id="m_name2" name="m_name2" placeholder="Name" class="form-control">
                        </div>
                    </div>

                    <div class="col-md-3 col-sm-12  form-group">
                        <div class="form-group row">
                            <label>Email of Member-2 *</label>
                            <input required="required" type="email" id="m_email2" name="m_email2" placeholder="Email" class="form-control">
                        </div>
                    </div>

                    <div class="col-md-3 col-sm-12  form-group">
