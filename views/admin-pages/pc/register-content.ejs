<div class="right_col" role="main">
    <div class="">
      <div class="clearfix"></div>
      <div class="row">
        <div class="col-md-12 col-sm-12 ">
          <div class="x_panel">
            <div class="x_title">
              <h2>Programming Contest  <small>Team Registration</small></h2>
              <ul class="nav navbar-right panel_toolbox">
                <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>
              </ul>
              <div class="clearfix"></div>
            </div>
            <div class="x_content">
              <br/>

              <%if(error.length>0){if(error == "Team has been registered succesfully!"){%>
                    <div class="alert alert-success">
                        <%= error%><br />
                    </div>
                    <%}else{%>
                    <div class="alert alert-danger">
                        <%= error%><br />
                    </div>
                    <%}%>
                <%}%>
  
              <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left" 
                method="POST" action="/ProgrammingContest/register">
                
                <!--Team-->
                <div class="x_title">
                  <h6>Team Information</h6>                    
                  <div class="clearfix"></div>
                </div>

                <div class="item form-group">
                    <div class="col-md-6 col-sm-12  form-group">
                        <div class="form-group row">
                            <label>Team Name *</label>
                            <input required="required" type="text" id="teamName" name="teamName" placeholder="Team Name" class="form-control">
                        </div>
                    </div>

                    <div class="col-md-6 col-sm-12  form-group">
                        <div class="form-group row">
                            <label>Institution Name *</label>
                            <input required="required" type="text" id="institution" name="institution" placeholder="Institution Name" class="form-control">
                        </div>
                    </div>
                </div>

                <!--Coach-->
                <div class="x_title">
                    <h6>Coach Information</h6>                    
                    <div class="clearfix"></div>
                </div>

                <div class="item form-group">
                    <div class="col-md-3 col-sm-12  form-group">
                        <div class="form-group row">
                            <label>Name of Coach *</label>
                            <input required="required" type="text" id="coachName" name="coachName" placeholder="Name" class="form-control">
                        </div>
                    </div>

                    <div class="col-md-3 col-sm-12  form-group">
                        <div class="form-group row">
                            <label>Email of Coach *</label>
                            <input required="required" type="email" id="coachEmail" name="coachEmail" placeholder="Email" class="form-control">
                        </div>
                    </div>

                    <div class="col-md-3 col-sm-12  form-group">
                        <div class="form-group row">
                            <label>Contact No. of Coach *</label>
                            <input required="required" type="text" id="coachContact" name="coachContact" placeholder="Contact" class="form-control">
                        </div>
                    </div>

                    <div class="col-md-3 col-sm-12  form-group">
                        <div class="form-group row">
                            <label>T-shirst Size *</label>
                            <select class="form-control" id="coachTshirt" name="coachTshirt">
                                <option disabled>Choose an Option</option>
                                <option value="M">Medium</option>
                                <option value="XL">Extra Large</option>
                                <option value="L">Large</option>
                                <option value="S">Small</option>
                            </select>
                        </div>
                    </div>
                </div>

                <!--Team Member 0-->
